generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Meme {
  id   Int    @id @default(autoincrement())
  name String
}

model Post {
  id       Int     @id @default(autoincrement())
  title    String
  content  String? 
}
